.container {
  position: relative;
}
.header {
  width: 100%;
  background: #2D99EE;
  background-image: -webkit-linear-gradient(top, #2D99EE, #338AF1);
  background-image: -moz-linear-gradient(top, #2D99EE, #338AF1);
  background-image: -ms-linear-gradient(top, #2D99EE, #338AF1);
  background-image: -o-linear-gradient(top, #2D99EE, #338AF1);
  background-image: linear-gradient(to bottom, #2D99EE, #338AF1);
  padding: 30px 0;
  display: flex;
  justify-content: center;
  color: #fff;
  text-align:center;
}
.mileage {
  padding:0 30rpx;
}
.title {
  display:flex;
  color: #333;
}
.finish {
  text-align:right;
  color: #f00;
}
.createdDispatch {
  margin-top:100px;
  background:#3E85FF !important;
  border-radius:25px;
  width:90%;
}
.time {
  font-size:12px;
}
.mission_num {
  background: rgb(249, 247, 247);
  position:absolute;
  top:10px;
  width:90%;
  margin:0 5%;
  z-index:99;
  border-top-left-radius:12rpx;
  border-top-right-radius:12rpx;
  display: flex;
  text-align: center;
  font-size: 14px;
}
.mission_num>view {
  flex: 1;
  margin: 30rpx;
}
.num_type {
  color: rgb(57, 129, 255);
  margin-top:10rpx;
}
.mission_detail{
  background: #fff;
  margin-top:30px;
  padding:0 40rpx 0 70rpx;
}
.webyill {
  padding: 40rpx 0;
  font-size: 12px;
  color: rgb(74, 75, 79);
  border-bottom: 1rpx solid rgb(214, 214, 214);
}
.webyill>view {
  padding: 10rpx;
}
.webyill_num {
  float: right;
}